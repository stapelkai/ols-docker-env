{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "description": "Onboarding preset for use with Redwerkz's repos",
    "extends": ["github>redwerkz/.github"],
    "packageRules": [{
            "enabled": true,
            "matchDatasources": [
            "docker"
        ],
            "matchUpdateTypes": [
            "major"
        ]
    }]
}
